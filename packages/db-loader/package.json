{
  "name": "@truffle/db-loader",
  "description": "Utility for loading @truffle/db",
  "license": "MIT",
  "author": "tyler feickert <tyler.feickert@consensys.net>",
  "homepage": "https://github.com/trufflesuite/truffle/tree/master/packages/db-loader#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/truffle.git",
    "directory": "packages/db-loader"
  },
  "bugs": {
    "url": "https://github.com/trufflesuite/truffle/issues"
  },
  "version": "0.2.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ethereum",
    "Truffle",
    "Truffle Db"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:compile build:bundle",
    "build:compile": "ttsc -b .",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' webpack --config webpack.config.ts",
    "prepare": "yarn build"
  },
  "optionalDependencies": {
    "@truffle/db": "^2.0.6"
  },
  "devDependencies": {
    "@types/webpack": "~5.28.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  }
}

    "@types/ws": "^7.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.5.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.25.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",

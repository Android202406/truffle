    "prepare": "yarn build"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/debug": "^4.1.5",
    "@types/node": "~12.12.0",
    "@types/webpack": "~5.28.0",
    "async-retry": "^1.3.1",
    "axios": "0.27.2",
    "debug": "^4.3.1",
    "node-abort-controller": "^3.0.1",
    "typescript": "^4.7.4",
    "web3-utils": "1.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "ethereum",
    "etherscan",
    "ipfs",
    "solidity",
    "sourcify",

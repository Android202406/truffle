{
  "name": "@truffle/expect",
  "description": "Simple module for ensuring specific options are passed to a function",
  "license": "MIT",
  "author": "Tim Coulter <tim@trufflesuite.com>",
  "homepage": "https://github.com/trufflesuite/truffle/blob/master/packages/expect#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/truffle.git",
    "directory": "packages/expect"
  },
  "bugs": {
    "url": "https://github.com/trufflesuite/truffle/issues"
  },
  "version": "0.1.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:compile build:bundle",
    "build:compile": "ttsc -b .",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' webpack --config webpack.config.ts",
    "clean": "rm -rf ./dist",
    "prepare": "yarn build",
    "test": "run-s test:types test:code",
    "test:code": "mocha",
    "test:types": "tsd"
  },
  "devDependencies": {
    "@types/node": "~12.12.0",
    "@types/webpack": "~5.28.0",
    "mocha": "10.1.0",
    "tsd": "^0.15.1",
    "ttypescript": "1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "3.3.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "ethereum",
    "expect",
    "truffle"
  ],
  "publishConfig": {
    "access": "public"
  }
}

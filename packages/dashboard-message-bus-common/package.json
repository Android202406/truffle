{
  "name": "@truffle/dashboard-message-bus-common",
  "version": "0.1.4",
  "description": "Message types that are used when communicating with the Truffle dashboard message bus",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "run-s build:compile build:bundle",

{
  "name": "@truffle/resolver",
  "description": "Resolve contract dependencies given multiple configurable dependency sources",
  "license": "MIT",
  "author": "Tim Coulter <tim@trufflesuite.com>",
  "homepage": "https://github.com/trufflesuite/truffle/tree/master/packages/resolver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/truffle.git",
    "directory": "packages/resolver"
  },
  "bugs": {
    "url": "https://github.com/trufflesuite/truffle/issues"
  },
  "version": "9.0.23",
  "main": "dist/index.js",
  "files": [
    "dist",
    "solidity"
  ],
  "scripts": {
    "build": "run-s build:compile build:bundle",
    "build:compile": "ttsc -b .",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' webpack --config webpack.config.ts",
    "prepare": "yarn build",
    "test": "run-s test:ts test:js",
    "test:js": "mocha test/*.js",
    "test:ts": "mocha -r ts-node/register test/*.ts"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@truffle/compile-solidity": "^6.0.50",
    "@truffle/contract": "^4.6.7",
    "@truffle/contract-sources": "^0.2.0",
    "@truffle/expect": "^0.1.3",
    "@truffle/provisioner": "^0.2.67"
  },
  "devDependencies": {
    "@types/node": "~12.12.0",
    "@types/sinon": "^9.0.10",
    "@types/webpack": "~5.28.0",
    "abi-to-sol": "^0.6.6",
    "debug": "^4.3.1",
    "detect-installed": "^2.0.4",
    "fs-extra": "^9.1.0",
    "get-installed-path": "^4.0.8",
    "glob": "^7.1.6",
    "mocha": "10.1.0",
    "sinon": "^9.0.2",
    "ts-node": "10.7.0",
    "ttypescript": "1.5.13",
    "typescript": "^4.7.4",
    "web3-utils": "1.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "dependencies",
    "ethereum",
    "resolver",
    "truffle"
  ],
  "publishConfig": {
    "access": "public"
  }
}

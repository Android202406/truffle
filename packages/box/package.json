{
  "name": "@truffle/box",
  "description": "Truffle project boilerplate utility",
  "license": "MIT",
  "author": "g. nicholas d'andrea <gnidan@trufflesuite.com>",
  "homepage": "https://github.com/trufflesuite/truffle/tree/master/packages/box#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/truffle.git",
    "directory": "packages/box"
  },
  "bugs": {
    "url": "https://github.com/trufflesuite/truffle/issues"
  },
  "version": "2.1.64",
  "main": "dist/index.js",
  "scripts": {
    "build": "run-s build:compile build:bundle",
    "build:compile": "ttsc -b .",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8192' webpack --config webpack.config.ts",
    "prepare": "yarn build",
    "test": "mocha --require source-map-support/register --timeout 10000"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@truffle/config": "^1.3.44"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "9.0.1",
    "@types/tmp": "^0.1.0",
    "@types/webpack": "~5.28.0",
    "axios": "0.27.2",
    "debug": "^4.3.1",
    "download-git-repo": "^3.0.2",
    "fs-extra": "^9.1.0",
    "ignore": "^5.1.8",
    "inquirer": "8.2.4",
    "mocha": "10.1.0",
    "sinon": "^9.0.2",
    "tmp": "^0.2.1",
    "ts-node": "10.7.0",
    "typescript": "^4.7.4",
    "vcsurl": "^0.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
